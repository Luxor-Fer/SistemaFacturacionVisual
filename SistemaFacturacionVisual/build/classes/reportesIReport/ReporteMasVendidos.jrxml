	<queryString language="SQL">
		<![CDATA[SELECT
     P.`ID_PRO` AS CODIGO,
     P.`NOM_PRO` AS NOMBRE,
     P.`PRE_PRO` AS PRECIO,
     SUM(DP.`CANTIDAD`) AS TOTAL_VENDIDOS
FROM
     productos P, detalle_productos DP
